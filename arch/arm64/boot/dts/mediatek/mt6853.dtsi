			reg = <0 0x18002000 0 0x1000>,
				/*TOP_RGU_BASE */
			    <0 0x10007000 0 0x0100>,
				/*INFRACFG_AO_BASE */
			    <0 0x10001000 0 0x1000>,
				/*SPM_BASE */
			    <0 0x10006000 0 0x1000>,

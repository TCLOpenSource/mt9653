	pm_pinctrl: pinctrl@1c022000 {
		compatible = "mediatek,mt5896-pinctrl";
		reg = <0x0 0x1c022000 0x0 0x200>;
		skip-str-restore;
		#pinctrl-cells = <2>;
		#gpio-range-cells = <0x3>;
		pinctrl-mt5896,bit-per-mux;
		pinctrl-mt5896,register-width = <8>;
		pinctrl-mt5896,function-mask = <0xf>;
		pinctrl-mt5896,npins = <97>;
		/* pin base, nr pins & gpio function */
		pinctrl-mt5896,gpio-range = <
			&pm_range 0 97 0
		>;
		pm_range: gpio-range {
			#pinctrl-mt5896,gpio-range-cells = <3>;
		};

		pm_uart_pmux: pm_uart_pmux {
		};

		pm_led_pwm_pmux: pm_led_pwm_pmux {
		};


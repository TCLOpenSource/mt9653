	gpio0: gpio@1c2e6000 {
		compatible = "mediatek,mt5896-gpio";
		reg = <0x0 0x1c2e6000 0x0 0x200>, /* gpio */
		      <0x0 0x1c2e6a00 0x0 0x200>; /* gpio intr */
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupt-parent = <&mtk_intc0_fiq>;
		/* <gpio irq>, <gpio fiq> */
		interrupts = <0x0 0x3e 0x4>,
			     <0x0 0x3f 0x4>;
		gpio-controller;
		#gpio-cells = <2>;
		gpio-ranges = <&pinctrl 0 0 139>;
		ngpios = <139>;
		base = <0>;
	};

	pm_gpio0: gpio@1c023000 {
		compatible = "mediatek,mt5896-gpio";
		reg = <0x0 0x1c023000 0x0 0x200>, /* gpio */
		      <0x0 0x1c022e00 0x0 0x200>; /* gpio intr */
		skip-str-restore;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupt-parent = <&mtk_intc2_fiq_high>;
		/* <gpio irq>, <gpio fiq> */
		interrupts = <0x0 0x18 0x4>,
			     <0x0 0x19 0x4>;
		gpio-controller;
		#gpio-cells = <2>;
		gpio-ranges = <&pm_pinctrl 0 0 97>;
		ngpios = <97>;
		base = <200>;
	};
	gpio_front_led: front_leds {
		compatible = "gpio-leds";
	};
	gpio_breath_led: breath_led_0 {
		compatible = "breath-leds";
		//mboxes = <&mailbox_to_cm4 MBOX_CHAN_ID_LED>;
		mboxes = <&mailbox_to_pm51 MBOX_CHAN_ID_LED>;
	};

	pinctrl_narcos: pinctrl_narcos {
		compatible = "mediatek,mt5896-pinctrl-narcos";
	};

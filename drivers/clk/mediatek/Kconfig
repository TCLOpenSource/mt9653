# MediaTek Clock Drivers
#
menu "Clock driver for MediaTek SoC"
	depends on ARM || ARM64 || COMPILE_TEST

config COMMON_CLK_MT5870
	tristate "Clock driver for MediaTek TV MT5870"
	depends on (MSTAR_CHIP && ARM64) || COMPILE_TEST
	select COMMON_CLK_MEDIATEK_TV
	help
	  This driver supports MediaTek MT5870 basic clocks.
	  Support SoC family and have data saperated to DTS
	  More info is in Documentation/devicetree/bindings/
	  clock/mediatek/clk-mt5870.txt

config COMMON_CLK_MT5896
	tristate "Clock driver for MediaTek TV MT5896"
	depends on MSTAR_CHIP || COMPILE_TEST
	select COMMON_CLK_MEDIATEK_TV
	help
	  This driver supports MediaTek MT5896 basic clocks.
	  Support SoC family and have data saperated to DTS
	  More info is in Documentation/devicetree/bindings/
	  clock/mediatek/clk-mt5896.txt

config COMMON_CLK_MEDIATEK_TV
	tristate "MEDIATEK TV"
	help
	  MediaTek TV SoCs' clock support.
	  Inform TV specific clock component

config COMMON_CLK_MEDIATEK_DBG
	tristate "MEDIATEK Debug"
	depends on COMMON_CLK_MEDIATEK_TV
	help
	  Debug for MediaTek TV SoCs' clock.

config COMMON_CLK_MEDIATEK
	tristate
	select RESET_CONTROLLER
	---help---
	  MediaTek SoCs' clock support.

